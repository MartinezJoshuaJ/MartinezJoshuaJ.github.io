
h1{
    background-color:#013C65 ;
    font-size: 30px;
	text-align: center;
}
.content {
  background-color: #013C65;
  padding: 1px;
  text-align: center;
}

body {
	
	text-align: center;
	background-color:#013C65;
	margin: auto;
	font-family: Copperplate, Papyrus, fantasy;
	font-weight: bold;
}
.topnav a {
  display: block;
  color: #F6B000;
  background-color: #013C65;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
}

/* Change color on hover */
.topnav a:hover {
  background-color: #F6B000;
  color: black;
}
.button {
  border: 2px solid #F6B000;
  color: #013C65;
  padding: 16px 32px;
  text-align: center;
  text-decoration: none;
  display: inline-block;
  font-size: 18px;
  margin: 2px 2px;
  transition-duration: 0.4s;
  cursor: pointer;
  background-color: #F6B000;
  font-family: Copperplate, Papyrus, fantasy
}

a, a:link, a:visited {
  color: #F6B000;
  background-color: transparent;
  text-decoration: none;
}
.button:hover {
  background-color: #837543;
  color: #013C65;
}

.footer {
	background-color: #013C65;
	margin: auto;
}
p{
	text-align: center;
}
ul {
	text-align: center;
}
caption {
	font-size: 32px;
	font-family: Copperplate, Papyrus, fantasy;
	text-align: center;
}
th {
	width: 200px;
	font-size: 20px;
	color: black;
	text-align: center;
}
	

th, td
{
	border: 1px solid #F6B000;
	padding-left: 10px;
	padding-right: 10px;
	padding-top: 10px;
	padding-bottom: 10px;
	color: #F6B000;
	font-family: Copperplate, Papyrus, fantasy;
	text-align: center;
	
}
table
{
	border-spacing: 0px;
	text-align: center;
}

/* Create three equal columns that floats next to each other */
.column {
  float: left;
  width: 33.33%;
}

/* Clear floats after the columns */
.row:after {
  content: "";
  display: table;
  clear: both;
}

/* Responsive layout - makes the three columns stack on top of each other instead of next to each other */
@media screen and (max-width:600px) {
  .column {
    width: 100%;
  }
}
img {
  border-radius: 40%;
}