
* {
  box-sizing: border-box;
  font-family: Arial, Helvetica, sans-serif;
}

body {
  margin: 1;
  font-family: Arial, Helvetica, sans-serif;
  text-align: center;
}

/* Style the top navigation bar */
.topnav {
  overflow: hidden;
  background-color: #013C65;
}

/* Style the topnav links */
.topnav a {
  float: center;
  display: block;
  color: #F6B000;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
}

/* Change color on hover */
.topnav a:hover {
  background-color: #F6B000;
  color: black;
}

/* Style the content */
.content {
  background-color: #013C65;
  padding: 10px;
  text-align: center;
}

/* Style the footer */
.footer {
  background-color: #F6B000;
}
